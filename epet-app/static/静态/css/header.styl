/*头部样式*/
.seacher{
  width: 100%;
  height: 41px;
  padding: 5px 10px;
  .epet-category
    float: right;
  .search-text input
    width: 100%;
    border: 0;
    height: 1rem;
    background: #e9e9e9;
    border-radius: 4px;
    color: #bcbcbc;
    text-indent: 10px;
    font-size: 13px;
    outline: 0;

}



.epet-category>img {
  width: 1rem;
  height: 1rem;
}
/*清除浮动*/
.clearfix{
*zoom: 1;
}
.clearfix::after{
  content: '';
  display: block;
  clear: both;
}
